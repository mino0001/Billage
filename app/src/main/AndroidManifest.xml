<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/icon_billage"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_billage"
        android:supportsRtl="true"
        android:theme="@style/Theme.billage"
        tools:targetApi="31"
        >
        <activity
            android:name=".HomeuserActivity"
            android:exported="false"
            android:label="@string/title_activity_homeuser"
            android:theme="@style/Theme.billage" />

        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.billage" />
        <activity
            android:name=".ChpwdActivity"
            android:exported="false"
            android:label="@string/title_activity_chpwd"
            android:theme="@style/Theme.billage" />
        <activity
            android:name=".SignupActivity"
            android:exported="false"
            android:label="@string/title_activity_signup"
            android:theme="@style/Theme.billage" />
        <activity
            android:name=".Send2Activity"
            android:exported="false"
            android:label="@string/title_activity_send2"
            android:theme="@style/Theme.billage" />
        <activity
            android:name=".NotiActivity"
            android:exported="false"
            android:label="@string/title_activity_noti"
            android:theme="@style/Theme.billage" />
        <activity
            android:name=".EditCategoryActivity"
            android:exported="false"
            android:label="@string/title_activity_category"
            android:theme="@style/Theme.billage" />
        <activity
            android:name=".GoodsinfoActivity"
            android:exported="false"
            android:label="@string/title_activity_goodsinfo"
            android:theme="@style/Theme.billage" />
        <activity
            android:name=".CheckrentActivity"
            android:exported="false"
            android:label="@string/title_activity_checkrent"
            android:theme="@style/Theme.billage" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>